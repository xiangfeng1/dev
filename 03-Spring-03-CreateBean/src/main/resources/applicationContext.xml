<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!--无参构造-->
   <bean id="student" class="Student"></bean>
    <!--创建工厂-->
    <bean id="studentFactory" class="com.example.factory.StudentFactory"></bean>
    <bean id="student2" factory-bean="studentFactory" factory-method="getStudent"></bean>
  <!--静态工厂-->
    <bean id="student3" class="com.example.factory.StudentStaticFactory" factory-method="getStudent"></bean>
   </beans>